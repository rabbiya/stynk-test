# Model settings
LLM_MODEL=gpt-4  # or your preferred model
LLM_PROVIDER=openai
LLM_TEMPERATURE=0  # 0 for deterministic output

# BigQuery settings
BIGQUERY_PROJECT_ID=your-project-id
BIGQUERY_DATASET=your-dataset

# API settings
OPENAI_API_KEY=your-openai-api-key

# Query settings
MAX_BYTES_BILLED=500000000  # 500MB default
QUERY_TIMEOUT=30  # 30 seconds default 